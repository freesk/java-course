package set_06_project_04;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr = { 2,3,4,3,2,6,3,6,8,2,9 };
		
		int i = 0;
		int j = 0;
		int size = arr.length;
		
		int temp;

		while(true) {
			finished = true;
			for(i = 0; i < size - 1; i++) {
				if(arr[i] > arr[i+1]) {
					temp = arr[i+1];
					arr[i+1] = arr[i];
					arr[i] = temp;
					finished = false;
				}
			}
		}
		
		
	}

}
