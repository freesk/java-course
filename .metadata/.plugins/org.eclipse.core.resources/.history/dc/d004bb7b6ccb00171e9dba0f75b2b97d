public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(formatNumber(1234567.892));
	}
	
	private static String formatNumber(double num, boolean isCurrency = false) {
		String res = Double.toString(num);
		
		return res;
	}

}
